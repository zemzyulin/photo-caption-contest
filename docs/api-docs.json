                        "application/json": {
                            "schema": {
                                "$ref": "#/components/req_user"
                            },
                            "example": {"username":"test_user1","password":"p@ssword1"}
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/user"
