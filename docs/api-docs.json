        "/users/login": {
            "post": {
                "description": "Authenticate user",
                "summary": "Authenticates user and responds with JWT in header",
                "requestBody": {
                    "description": "Username and password",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/req_user"
                            },
                            "example": {"username":"test_user1","password":"p@ssword1"}
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "description": "User ID",
                                            "type": "integer"
                                        },
                                        "username": {
                                            "description": "User's name",
                                            "type": "string"
                                        },
                                        "token": {
                                            "description": "JWT assigned to user",
                                            "type": "string"
                                        }
                                    } 
                                },
                                "headers": {
                                    "authorization": {
                                        "description": "JWT assigned to user",
                                        "schema": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "example": {
                                    "id": 1,
                                    "username": "test_user1",
                                    "token":"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6OCwidXNlcm5hbWUiOiJ0ZXN0X3VzZXJfOCIsImlhdCI6MTY2MTg1MzQ3OCwiZXhwIjoxNjYxODU3MDc4fQ.-XKL1ZlQHpmUH2egBSPYFmX9TvcFwS2dTiUjiJIM5UE"    
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Incorrect password"
                    },
                    "404": {
                        "description": "Incorrect username"
                    }
                },
                "tags": ["User Login"]
            }
        },
