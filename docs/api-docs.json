                    },
                    "404": {
                        "description": "User not found"
                    },
                    "400": {
                        "description": "Error"
                    }
                },
                "tags": ["Users"]
            },
            "put": {
                "description": "Updates user based on ID",
                "summary": "Update user by ID",
                "parameters": [{
                    "name": "id",
                    "in": "path",
                    "description": "ID of user",
