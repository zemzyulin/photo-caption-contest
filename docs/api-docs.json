        "/users/{id}": {
            "get": {
                "description": "Returns user based on ID",
                "summary": "Find user by ID",
                "parameters": [{
                    "name": "id",
                    "in": "path",
                    "description": "ID of user",
                    "required": true,
                    "schema": {
                        "type": "integer"
                    },
                    "example": 1
                }],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "description": "User ID",
                                            "type": "integer"
                                        },
                                        "username": {
                                            "description": "User's name",
                                            "type": "string"
                                        },
                                        "password": {
                                            "description": "Hashed user's password",
                                            "type": "string"
                                        },
                                        "updatedAt": {
                                            "description": "Timestamp of last update",
